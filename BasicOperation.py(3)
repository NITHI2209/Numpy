import numpy as np
arr = np.array([10,20,30])
arr2d = np.array([[10,20,30],[40,50,60]])
print(type(arr))
# <class 'numpy.ndarray'>
print(arr.size) # number of elements 
print(arr2d.size)
# 3
# 6
print(arr.shape) # to identify shape 
print(arr2d.shape)
# (3,)
#(2, 3)    ---> 2- no.of rows , 3 - no.of columns

print(arr.ndim) # to identify dimension
print(arr2d.ndim)
#1
#2
